      "value": "dev"
    },
    "location": {
      "value": "centralus"
    },
    "addressPrefix": {
      "value": "10.0.0.0/16"
    },
    "subnets": {
      "value": [
        {
          "name": "app-subnet",
          "addressPrefix": "10.0.1.0/24"
        },
        {
          "name": "db-subnet",
          "addressPrefix": "10.0.2.0/24"
        }
      ]
    },
    "enableDdosProtection": {
      "value": false
    },
    "additionalTags": {
      "value": {
        "costCenter": "Engineering",
        "project": "LAMP-Stack"
      }
    }
  }
